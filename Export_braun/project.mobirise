                      },
                      ".mbr-iconfont": {
                        "font-size": "38px",
                        "transition": "all 0.3s ease-in-out",
                        "color": "@icon",
                        "@media (max-width: 992px)": {
                          "font-size": "28px"
                        }
                      }
                    },
                    "&:hover": {
                      ".icon-wrapper": {
                        "background-color": "@icon",
                        ".mbr-iconfont": {
                          "color": "@activeColor"
                        }
                      }
                    },
                    "&:not(.collapsed)": {
                      ".icon-wrapper": {
                        "transform": "rotate(-45deg)"
                      }
                    }
                  }
                },
                ".panel-collapse": {
                  ".panel-body": {
                    "margin": "30px 0 0 0",
                    "@media (max-width: 992px)": {
                      "margin": "20px 0 0 0"
                    },
                    ".panel-text": {
                      "margin-bottom": "0"
                    }
                  }
                }
              }
            },
            ".mbr-section-title, .title-wrapper": {
